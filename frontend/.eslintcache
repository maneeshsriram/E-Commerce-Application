nonce = data.nonce;\n      const paymentData = {\n        paymentMethodNonce: nonce,\n        amount: getAmount(),\n      };\n\n      processPayment(userId, token, paymentData)\n        .then((response) => {\n          console.log(\"POINT-1\", response);\n\n          if (response.error) {\n            if (response.code == \"1\") {\n              console.log(\"PAYMENT Failed!\");\n              signout(() => {\n                return <Redirect to=\"/\" />;\n              });\n            }\n          } else {\n            setInfo({ ...info, success: response.success, loading: false });\n            console.log(\"PAYMENT SUCCESS\");\n\n            let product_names = \"\";\n            products.forEach(function (item) {\n              product_names += item.name + \", \";\n            });\n\n            const orderData = {\n              products: product_names,\n              transaction_id: response.transaction.id,\n              amount: response.transaction.amount,\n            };\n\n            createOrder(userId, token, orderData)\n              .then((response) => {\n                if (response.error) {\n                  if (response.code == \"1\") {\n                    console.log(\"Order Failed!\");\n                    signout(() => {\n                      return <Redirect to=\"/\" />;\n                    });\n                  }\n                } else {\n                  if (response.success == true) {\n                    console.log(\"ORDER PLACED!!\");\n                  }\n                }\n              })\n              .catch((error) => {\n                setInfo({ loading: false, success: false });\n                console.log(\"Order FAILED\", error);\n              });\n\n            cartEmpty(() => {\n              console.log(\"Did we got a crash?\");\n            });\n\n            setReload(!reload);\n          }\n        })\n\n        .catch((error) => {\n          setInfo({ loading: false, success: false });\n          console.log(\"PAYMENT FAILED\", error);\n        });\n    });\n  };\n\n\n\n  const showbtnDropIn = () => {\n    return (\n      <div>\n        {info.clientToken !== null && products.length > 0\n          ? (\n            <div>\n              <DropIn\n                options={{ authorization: info.clientToken }}\n                onInstance={(instance) => (info.instance = instance)}\n              >\n              </DropIn>\n              <button\n                onClick={onPurchase}\n                className=\"btn btn-block btn-success\"\n              >\n                Buy Now\n              </button>\n            </div>\n          )\n          : (\n            <h3>Please login first or add something in cart</h3>\n          )}\n      </div>\n    );\n  };\n\n\n\n  return (\n    <div>\n      <h3>Your bill is $ {getAmount()}</h3>\n      {showbtnDropIn()}\n    </div>\n  );\n};\n\nexport default PaymentB;\n","C:\\Users\\manee\\Desktop\\Development\\1. Web projects\\django-react\\frontend\\src\\core\\helper\\paymentHelper.js",[],"C:\\Users\\manee\\Desktop\\Development\\1. Web projects\\django-react\\frontend\\src\\core\\helper\\ImageHelper.js",[],"C:\\Users\\manee\\Desktop\\Development\\1. Web projects\\django-react\\frontend\\src\\core\\helper\\orderHelper.js",[],{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","replacedBy":"91"},{"ruleId":"92","severity":1,"message":"93","line":32,"column":6,"nodeType":"94","endLine":32,"endColumn":8,"suggestions":"95"},{"ruleId":"96","severity":1,"message":"97","line":4,"column":26,"nodeType":"98","messageId":"99","endLine":4,"endColumn":31},{"ruleId":"88","replacedBy":"100"},{"ruleId":"90","replacedBy":"101"},{"ruleId":"96","severity":1,"message":"102","line":19,"column":11,"nodeType":"98","messageId":"99","endLine":19,"endColumn":15},{"ruleId":"96","severity":1,"message":"103","line":19,"column":59,"nodeType":"98","messageId":"99","endLine":19,"endColumn":70},{"ruleId":"92","severity":1,"message":"104","line":53,"column":6,"nodeType":"94","endLine